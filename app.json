{
  "expo": {
    "name": "AiChat",
    "slug": "AiChat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "AiChat",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "image": "./assets/images/logo.png",
      "backgroundColor": "#1a1a2e"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#1a1a2e"
      },
      "package": "com.hadirasal.AiChat"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "aec94f7f-a9a9-4fa3-89d1-f4db1710ee8e"
      }
    }
  }
}
